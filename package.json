{
  "type": "module",
  "scripts": {
    "board": "cd board-client && npm start",
    "dashboard": "cd dashboard-client && npm start",
    "master": "cd masterClient && npm start",
    "specs": "cd specs-client && npm start",
    "members": "cd members-client && npm start",
    "calender": "cd calender-client && npm start",
    "settings": "cd settings-client && npm start",
    "roles": "cd roles-client && npm start",
    "server": "cd Server && nodemon index.js",
    "start": "concurrently \"npm run board\" \"npm run dashboard\" \"npm run master\" \"npm run specs\" \"npm run members\" \"npm run calender\" \"npm run settings\" \"npm run roles\" ",
    "installBoard": "cd board-client && npm i",
    "installDashboard": "cd dashboard-client && npm i",
    "installMaster": "cd masterClient && npm i",
    "installSpecs": "cd specs-client && npm i",
    "installMembers": "cd members-client && npm i",
    "installCalender": "cd calender-client && npm i",
    "installSettings": "cd settings-client && npm i",
    "installRoles": "cd roles-client && npm i",
    "installServer": "cd Server && npm i",
    "deployBoard": "cd board-client && npm run deploy",
    "deployDashboard": "cd dashboard-client && npm run deploy",
    "deployMaster": "cd masterClient && npm run deploy",
    "deploySpecs": "cd specs-client && npm run deploy",
    "deployRoles": "cd roles-client && npm run deploy",
    "deployMembers": "cd members-client && npm run deploy",
    "deployServer": "cd Server && npm run deploy",
    "deployCalender": "cd calender-client && npm run deploy",
    "installAll": "concurrently \"npm run installBoard\" \"npm run installDashboard\" \"npm run installMaster\" \"npm run installSpecs\" \"npm run installMembers\" \"npm run installCalender\" \"npm run installSettings\" \"npm run installRoles\" \"npm run installServer\" ",
    "deployAll": "concurrently \"npm run deployBoard\" \"npm run deployDashboard\" \"npm run deployMaster\" \"npm run deploySpecs\" \"npm run deployMembers\" \"npm run deployServer\" \"npm run deployCalender\""
  },
  "dependencies": {
    "body-parser": "^1.20.0",
    "concurrently": "^7.3.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "mongoose": "^6.5.1",
    "nodemon": "^2.0.19",
    "redux-micro-frontend": "^1.3.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "eslint": "^8.19.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-styled-components-a11y": "^0.0.40",
    "prettier": "^2.7.1"
  }
}
